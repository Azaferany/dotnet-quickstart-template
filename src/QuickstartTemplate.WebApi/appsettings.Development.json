{
  "ConnectionStrings": {
    "Redis": ""
  },
  "Sentry": {
    "Dsn": "",
    "AttachStackTrace": true,
    "IncludeActivityData": true,
    "Debug": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
