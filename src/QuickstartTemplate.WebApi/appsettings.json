{
  "ConnectionStrings": {
    "Redis": "",
    "DefaultConnection": ""
  },
  "Sentry": {
    "Dsn": "",
    "AttachStackTrace": true,
    "IncludeActivityData": true,
    "Debug": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Sentry"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithSpan"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@x is not null and TypeOf(@x) like '%OperationCanceledException%'"
        }
      }
    ]
  },
  "Authentication" : {
    "Authority" : "",
    "Audience" : "",
    "ClientId" : "",
    "ClientSecret" : ""
  },
  "LogRequestResponse": {
    "LogMode": "LogFailures",
    "ResponseBodyLogMode": "LogAll"
  },
  "HttpLogging": {
    "LoggingFields": "None"
  },
  "AllowedHosts": "*"
}
